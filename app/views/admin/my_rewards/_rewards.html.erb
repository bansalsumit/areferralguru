      <%= render 'coupan_rewards', locals: {section_name: section_name, program: program, section_type: section_type} %>
    </div>
  <div class="accordion-item">
    <%= render 'cash_rewards', locals: {section_name: section_name.to_s, program: program, section_type: section_type} %>
